<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
  <Description/>
  <FaultRules/>
  <PreFlow name="PreFlow">
    <Request/>
    <Response>
      <Step>
        <Name>External-Callout</Name>
      </Step>
    </Response>
  </PreFlow>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <Flows/>
  <HTTPProxyConnection>
    <BasePath>/v1/samples/external-callout</BasePath>
    <Properties/>
  </HTTPProxyConnection>
  <RouteRule name="noroute"/>
</ProxyEndpoint>
